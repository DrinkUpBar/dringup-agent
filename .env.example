# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
# OPENAI_BASE_URL=https://api.openai.com/v1  # Optional: Custom API endpoint

# Mem0 Configuration
MEM0_API_KEY=your_mem0_api_key_here
MEM0_BASE_URL=https://api.mem0.ai

# Milvus Configuration for Mem0 Vector Store (Primary storage)
# Milvus provides persistent vector storage for memories
MILVUS_URL=http://localhost:19530  # Milvus server URL
MILVUS_TOKEN=  # Optional: Authentication token for Milvus (leave empty for local setup)
MILVUS_COLLECTION_NAME=mem0  # Collection name in Milvus
MILVUS_DB_NAME=  # Optional: Database name (leave empty for default)

# OpenAI Embedding Configuration for Mem0
EMBEDDING_MODEL=text-embedding-3-large
EMBEDDING_DIMS=1536  # Dimensions for embedding model

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8001

# DrinkUp Backend Configuration
DRINKUP_BACKEND_URL=http://localhost:8080

# Redis Configuration (for conversation memory)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# MySQL Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=drinkup
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password_here